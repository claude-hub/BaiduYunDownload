{
  "name": "adress-book-app",
  "version": "0.0.0",
  "description": "Address book app, where you can search for users addresses and personal information.",
  "main": "./dist/index.ts",
  "scripts": {
    "start": "npm run build && node ./server.js",
    "start:dev": "webpack-dev-server --config ./webpack/webpack.dev.js",
    "build": "webpack --config ./webpack/webpack.prod.js",
    "test": "jest ./src --passWithNoTests --config ./config/jest/jest.config.js",
    "test:watch": "jest ./src --watch --passWithNoTests --config ./config/jest/jest.config.js",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "npx eslint --ext .ts,.tsx ./src",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:scss": "npx sass-lint -v",
    "lint:scss:fix": "npx sass-lint-auto-fix --config-sass-lint .sass-lint.yml",
    "prettier": "npx prettier --write './src/**/*.{js,jsx,ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://stanislav-halyn@github.com/stanislav-halyn/adress-book-app.git"
  },
  "author": "Stanislav Halyn <stasgalin@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stanislav-halyn/adress-book-app/issues"
  },
  "homepage": "https://github.com/stanislav-halyn/adress-book-app#readme",
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@types/classnames": "2.2.10",
    "@types/enzyme": "3.10.5",
    "@types/jest": "26.0.10",
    "@types/ramda": "0.27.14",
    "@types/react": "16.9.46",
    "@types/react-css-modules": "4.6.2",
    "@types/react-dom": "16.9.8",
    "@types/react-loader": "2.4.4",
    "@types/react-redux": "7.1.9",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.5",
    "@types/react-virtualized-auto-sizer": "1.0.0",
    "@types/react-window": "1.8.2",
    "@types/react-window-infinite-loader": "1.0.3",
    "@types/redux-mock-store": "1.0.2",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.0.3",
    "css-loader": "4.2.1",
    "cz-customizable": "6.3.0",
    "dotenv-webpack": "2.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.3",
    "eslint": "7.7.0",
    "eslint-config-airbnb-typescript": "9.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "mini-css-extract-plugin": "0.10.0",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "1.5.4",
    "sass-lint": "1.13.1",
    "sass-lint-auto-fix": "0.21.0",
    "sass-loader": "9.0.3",
    "ts-jest": "26.2.0",
    "ts-loader": "8.0.2",
    "typescript": "3.9.7",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.4.0",
    "@testing-library/react": "10.4.9",
    "classnames": "2.2.6",
    "express": "4.17.1",
    "history": "5.0.0",
    "ramda": "0.27.1",
    "react": "16.13.1",
    "react-css-modules": "4.7.11",
    "react-dom": "16.13.1",
    "react-loader": "2.4.7",
    "react-redux": "7.2.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-virtualized-auto-sizer": "1.0.2",
    "react-window": "1.8.5",
    "react-window-infinite-loader": "1.0.5",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './config/git/commitlint.js'",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "engines": {
    "node": ">11.15.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  }
}
